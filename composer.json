{
    "name": "husail/http-iterator",
    "description": "Http Iterator",
    "keywords": [
        "http-iterator",
        "iterator"
    ],
    "homepage": "https://github.com/husail/http-iterator",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Danilo",
            "email": "victordanilo_cs@live.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "Husail\\HttpIterator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Husail\\HttpIterator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
